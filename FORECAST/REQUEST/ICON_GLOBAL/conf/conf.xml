<!--Create ICON class configuration file-->

<config>
    <class name="ICON_GLOBAL">
        <root_data value='https://opendata.dwd.de/weather/nwp/icon/grib/'/>
        <storage_format value='grib2.bz2'/> <!--format of the data-->
        <days_storage value='1'/> <!--days of storage-->
        <days_format value='YYYYMMDD'/> <!--format of the days-->
        <model_version name='icon'/>    <!--model version-->
        <coordinates step="0.125">
            <west value='-180.00'/> <!--coordinates of the model-->
            <east value='179.875'/>
            <south value='-90.00'/>
            <north value='90.00'/>
        </coordinates>
        <grid_type name='icosaedral'/> <!--grid type-->
            <horizontal_resolution value='13km'/> <!--resolution of the model-->
        <model name='icon-global'>
            <limit_last_day value='True'/> <!--limit last day-->
            <run_hours hour='00' delivery_hour='04' next_day='False'>
                <Hours_Steps> <!--Temporal resolution of the model -->
                    <step hours='1' min='0' max='78'/>
                    <step hours='3' min='81' max='180'/>
                </Hours_Steps>
            </run_hours>
            <run_hours hour='06' delivery_hour='10' next_day='False'>
